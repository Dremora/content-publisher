<div id=<%= name %>>
  <p><%= name %>:</p>

  <% linkables = LinkablesService.new("topical_event") %>
  <% if value&.any? %>
    <ul>
      <% value.map do |field| %>
        <li><%= linkables.by_content_id(field)["internal_name"] %></li>
      <% end %>
    </ul>
  <% else %>
    None
  <% end %>
</div>
